<h1>Here is the list</h1>

<% data["Search"].forEach(function(movie){%>

<% if(!data["Search"]){%>
    <% res.redirect("/")%>
<%}%>

<ul>
    <li>
        <strong>
            <%= movie.Title%>
        </strong>
        - <%= movie["Year"] %>
    </li>
</ul>
<% }); %>

<a href="/">Search again !</a>