<% if (data) { %>
<h1>Here is the list</h1>
    <ul>
    <% data.forEach(function(movie){ %>
        <li>
            <strong><%= movie.Title %></strong> - <%= movie.Year %>
        </li>
    <% }) %>
    </ul>
    
    <a href="/">Search Again!</a>
<% } else { %>
    <a href="/">Nothing found. Search Again!</a>
<% } %>